function [x_best, J_best] = grid_search(f, lb_I, ub_I, lb_P, ub_P, n_values)



% this function executes a grid search to find good starting points
% for the optimization

% author: Samuel Wiertz
% date: 05.01.2019

% inputs: lb_

J_best = inf;
x_best = zeros(2,1);

for ii = index(1,:)
    
    for kk = index(2,:)
        
        tic
        J = f([ii,kk]);
        toc
        if J > J_best
            
            J_best = J;
            x_best = [ii, kk];     
        end
    end
    
end





end

